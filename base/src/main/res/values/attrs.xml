<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LeftRightAlignTextView">
        <attr name="custom_left_text" format="string" />
        <attr name="custom_right_text" format="string" />
        <attr name="custom_right_icon" format="reference" />
        <attr name="custom_has_next_icon" format="boolean" />
        <attr name="custom_right_edit" format="boolean" />
        <attr name="custom_single_line" format="boolean" />
        <attr name="custom_keep_text_from_tv" format="boolean" />
        <attr name="custom_right_edit_max_length" format="integer" />
    </declare-styleable>
    <declare-styleable name="AutoMovableView">
        <attr name="custom_hide_time" format="integer" />
        <attr name="custom_show_time" format="integer" />
        <attr name="custom_duration" format="integer" />
    </declare-styleable>

    <declare-styleable name="FlexLayout">
        <attr name="custom_max_count" format="integer"/>
        <attr name="custom_view_horizontal_space" format="dimension" />
        <attr name="custom_view_vertical_space" format="dimension" />
    </declare-styleable>
    <declare-styleable name="CustomDateView">
        <attr name="custom_date_background_color" format="color" />
        <attr name="custom_date_text_size" format="dimension" />
        <attr name="custom_date_text_color" format="color" />
        <attr name="custom_tip_text" format="string" />
        <attr name="custom_tip_text_size" format="dimension" />
        <attr name="custom_tip_text_color" format="color" />
        <attr name="custom_separator_color" format="color" />
        <attr name="custom_date_background" format="reference" />
    </declare-styleable>
    <declare-styleable name="CollapsibleTextView">
        <attr name="custom_fold_tip" format="string" />
        <attr name="custom_unfold_tip" format="string" />
        <attr name="custom_unfold_icon" format="reference" />
        <attr name="custom_fold_icon" format="reference" />
        <attr name="custom_fold_tip_size" format="dimension" />
        <attr name="custom_tip_color" format="color" />
        <attr name="custom_fold_icon_width" format="dimension" />
        <attr name="custom_fold_icon_height" format="dimension" />
        <attr name="custom_unfold_icon_width" format="dimension" />
        <attr name="custom_unfold_icon_height" format="dimension" />
        <attr name="custom_fold_marginTop" format="dimension" />
        <attr name="custom_show_fold" format="boolean" />
    </declare-styleable>
    <declare-styleable name="RippleTextView">
        <attr name="custom_ripple_color" format="color" />
    </declare-styleable>
</resources>